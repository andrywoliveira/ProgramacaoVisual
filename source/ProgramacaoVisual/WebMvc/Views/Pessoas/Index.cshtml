@model WebMvc.Models.PessoaViewModel
@{
    ViewData["Title"] = "Eerro";
}
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome</th>
        </tr>
    </thead>
    <tbody>
    @foreach(var item in Model.Items)
    {
            <tr>
                <td>@item.Id</td>
                <td>@item.Nome</td>
            </tr>
    }
    </tbody>
</table>
